#!/bin/bash
procmgr stop tmo.cnf ami-worker_0_0
procmgr stop tmo.cnf ami-worker_0_1
procmgr stop tmo.cnf ami-worker_0_2
procmgr stop tmo.cnf ami-worker_0_3
procmgr stop tmo.cnf ami-node_0
procmgr stop tmo.cnf ami-manager
procmgr stop tmo.cnf ami-client
procmgr start tmo.cnf ami-worker_0_0
procmgr start tmo.cnf ami-worker_0_1
procmgr start tmo.cnf ami-worker_0_2
procmgr start tmo.cnf ami-worker_0_3
procmgr start tmo.cnf ami-node_0
procmgr start tmo.cnf ami-manager
procmgr start tmo.cnf ami-client
